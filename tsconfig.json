{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": true,
        "module": "es6",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "moduleResolution": "node",
        "baseUrl": "src",
        "paths": {
            "Pages/*": [ "Pages/*" ],
            "Components/*": [ "Components/*" ],
            "Utils/*": [ "Utils/*" ],
            "Img/*": [ "Img/*" ]
        }
    },
    "include": [
        "custom.d.ts",
        "src/**/*"
    ]
}